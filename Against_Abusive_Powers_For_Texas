        function myFunction2(){
         if (perps.value == "Law Enforcement" && hrs.value =="Physical Touch"){
        
          document.getElementById("demo3").innerHTML = "Physical Touch excessive force is a violation of the US Constitution - 18 U.S.C. 242; Title 8, color of law deprivation of rights; excessive force; Chapter 39 of the Texas Penal Code; a violation of Tex. Civ. Prac. & Rem. Code 101.021 (TTCA or Act) if abuse resulted in injury or death, file in district court. ";
          
            }
        else if (perps.value == "Law Enforcement" && hrs.value =="Energy Weapons") {
        
            document.getElementById("demo3").innerHTML = "Energy Weapon abuse (CED, DEW, Sound Weapon) by Law Enforcement is a violation of the US Constitution - 18 U.S.C. 242 color of law deprivation of rights; police miscondcut - Chapter 39 of the Texas Penal Code; a violation of Tex. Civ. Prac. & Rem. Code 101.021 (TTCA or Act) if abuse (wrongful act) resulted in injury or death, file in district court.";
        
        }
        else if (perps.value == "Law Enforcement" && hrs.value =="Intermediate Force") {
        
        document.getElementById("demo3").innerHTML = "Intermediate Force abuse by Law Enforcement is a violation of the US Constitution - 18 U.S.C. 242, color of law deprivation of rights; police miscondcut - Chapter 39 of the Texas Penal Code, a violation of Tex. Civ. Prac. & Rem. Code 101.021 (TTCA or Act) if abuse (wrongful act) resulted in injury or death, file in district court.";
        
        }
        else if (perps.value == "Contractor" && hrs.value =="Physical Touch") { 
        
        document.getElementById("demo3").innerHTML = "Contractor (Fed contractors et al.) abusing physical force is under disproportionate use of force, right to life/human rights violations, act of physical violence, file in superior court or district court under FTCA, See ruling: Edison v. U.S., supra, 822 F.3d 510, if contractor retained by the US Government.";
        
        }
        else if (perps.value == "Contractor" && hrs.value =="Energy Weapons") { 
        
        document.getElementById("demo3").innerHTML = "Contractor (Fed contractors et al.) abusing Energy Weapons is under disproportionate use of force, right to life/human rights violations, negligence, act of physical violence - RULE 392.321 (E), (3)(A) of Texas Administrative Code, file in superior court or district court under FTCA, see ruling: Edison v. U.S., supra, 822 F.3d 510, if contractor retained by the US Government.";
        
        }
        else if (perps.value == "Contractor" && hrs.value =="Intermediate Force") { 
        
        document.getElementById("demo3").innerHTML = "Contractor (Fed contractors et al.) abusing Intermediate Force Capabilities (IFC) is under disproportionate use of force, right to life/human rights violations, negligence, act of physical violence - RULE 392.321 (E), (3)(A) of Texas Administrative Code, file in superior court or district court under FTCA, See ruling: Edison v. U.S., supra, 822 F.3d 510, if contractor retained by the US Government.";
        
        }
        
        else if (perps.value == "Vigilante" && hrs.value =="Physical Touch") { 
        
        document.getElementById("demo3").innerHTML = "Vigilante group or individual physically abusing targeted person in targeted attack is disproportionate use of force, right to life/human rights violations, act of physical violence, acting without lawful authority, criminal penal code statute like Texas Statute Chapter 22: Assaultive Offenses, file in Texas Supreme Court.";
        
        }
        else if (perps.value == "Vigilante" && hrs.value =="Energy Weapons")  { 
        
        document.getElementById("demo3").innerHTML = "Vigilante abusing Energy Weapons (Laser/DEW/Sound Weapon) againt targeted person in targeted attack is disproportionate use of force because it invloves <q>radiant energy to achieve the same effect as blunt force...</q> according to the NIJ.gov (Overview of Less-Lethal Technologies, June 1, 2011 ), right to life/human rights violations, negligence, act of physical violence, without lawful authority, criminal penal code statute like Texas Statute Chapter 22: Assaultive Offenses, file in Texas Supreme Court.";
        
        }
        else if (perps.value == "Vigilante" && hrs.value =="Intermediate Force")  { 
        
        document.getElementById("demo3").innerHTML = "Vigilante person or group using Intermediate Force to abuse or harm targeted person in targeted attack is disproportionate use of force because it invloves <q>radiant energy to achieve the same effect as blunt force...</q> according to the NIJ.gov (Overview of Less-Lethal Technologies, June 1, 2011 ), right to life/human rights violations, negligence, act of physical violence, without lawful authority, criminal penal code statute like Texas Statute Chapter 22: Assaultive Offenses, file in Texas Supreme Court.";
        }
        
         else { document.getElementById("demo3").innerHTML = "Energy weapons technology like DEWs is emerging technology with a high potential for abuse due to its stealth nature, is called a gray zone by the military, litigation is needed to help establish a legal precedent in preventing DEW/DE IFC abuse.";
        }}
            
