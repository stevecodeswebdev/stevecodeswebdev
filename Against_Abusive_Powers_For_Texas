<script>
  function butAfter(){
  var text1 = document.getElementById("myTextt");
  var butt1 = document.getElementById("button");

  if (text1.value == null || text1.value == ""){
  butt1.style.backgroundColor = "white";
} else {
  butt1.style.backgroundColor = "#F22613";
}} 
</script>

